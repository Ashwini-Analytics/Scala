def check(a:Double, b:Double, k:(Double,Double)=> Double):Double = k(a,b)
val x = util.Random.nextInt()
val y = util.Random.nextInt()
val output1 = check(x,y, (a,b)=>a max b)
println("The greater number is:" + output1)
val output2 = check(x,y, (a,b)=>a min b)
println("The smaller number is:" + output2)
val output3 = check(x,y,(a,b)=>b)
println("The second number is:" + output3)
